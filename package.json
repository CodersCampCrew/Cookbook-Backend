{
  "name": "cookbook-backend",
  "version": "1.0.0",
  "description": "node-express-backend-app",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodersCampCrew/Cookbook-Backend.git"
  },
  "author": "KrisGum <krisgumularz@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0",
    "yarn": ">= 1.20.0"
  },
  "dependencies": {
    "express": "^4.17.3",
    "luxon": "^2.3.0",
    "mongoose": "^6.2.2",
    "yup": "^0.32.11"
